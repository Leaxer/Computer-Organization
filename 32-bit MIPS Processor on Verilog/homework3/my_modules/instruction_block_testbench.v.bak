module instruction_block_testbench();
  wire [31:0] read_data1;
  wire [31:0] read_data2;
  reg [31:0] write_data;
  reg [4:0] read_reg1;
  reg [4:0] read_reg2;
  reg [4:0] write_reg;
  reg regWrite;
	
	

	localparam period = 10;
	
	register_block UUT(read_data1, read_data2, write_data, read_reg1, read_reg2, write_reg, regWrite);
	
	initial
		begin
			#period;
			write_data = 32'd14;
			read_reg1 = 5'b00010;
			read_reg2 = 5'b00100;
			write_reg = 5'b00010;
			regWrite = 1'b1;
			$display ("Reg Write1 read-data1-%b, read-data2-%b, write_data-%b, read-reg1-%b, read_reg2-%b, write_reg-%b, regWrite-%b\n", read_data1, read_data2, write_data, read_reg1, read_reg2, write_reg, regWrite);
			#period;
			write_data = 32'd25;
			read_reg1 = 5'b00010;
			read_reg2 = 5'b00100;
			write_reg = 5'b00100;
			regWrite = 1'b1;
			$display ("Reg Write1 read-data1-%b, read-data2-%b, write_data-%b, read-reg1-%b, read_reg2-%b, write_reg-%b, regWrite-%b\n", read_data1, read_data2, write_data, read_reg1, read_reg2, write_reg, regWrite);
			#period;
			write_data = 32'b00000000000000111000000000000000;
			read_reg1 = 5'b00010;
			read_reg2 = 5'b00100;
			write_reg = 5'b00010;
			regWrite = 1'b1;
			$display ("Reg Write1 read-data1-%b, read-data2-%b, write_data-%b, read-reg1-%b, read_reg2-%b, write_reg-%b, regWrite-%b\n", read_data1, read_data2, write_data, read_reg1, read_reg2, write_reg, regWrite);
			#period;
			write_data = 32'b00000000000000111000000000000000;
			read_reg1 = 5'b00010;
			read_reg2 = 5'b00100;
			write_reg = 5'b00010;
			regWrite = 1'b0;
			$display ("Reg Write1 read-data1-%b, read-data2-%b, write_data-%b, read-reg1-%b, read_reg2-%b, write_reg-%b, regWrite-%b\n", read_data1, read_data2, write_data, read_reg1, read_reg2, write_reg, regWrite);
			end

endmodule